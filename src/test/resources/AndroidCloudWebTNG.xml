<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Android Web mobile testing Suite for EPAM Mobile Cloud">
  <parameter name="execution" value="cloud"/>
  <parameter name="appType" value="web"></parameter>
  <parameter name="udid" value="0A071FDD4005QL"/>
  <parameter name="platformName" value="Android"></parameter>
  <parameter name="browserName" value="Chrome"></parameter>
  <test name="Cloud Web Android tests">
    <groups>
      <run>
        <include name="web"/>
      </run>
    </groups>
    <packages>
      <package name="scenarios"/>
    </packages>
  </test>
</suite>
